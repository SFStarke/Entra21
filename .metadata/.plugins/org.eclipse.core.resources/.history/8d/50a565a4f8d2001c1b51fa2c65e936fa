	System.out.print("Telefonou para a vitima? ");
	res1 = read.next();
	System.out.print("\nEsteve no local do crime? ");
	res2 = read.next();
	System.out.print("\nMora perto da vitima? ");
	res3 = read.next();
	System.out.print("\nDevia para vitima? ");
	res4 = read.next();
	System.out.print("\nJá trabalhou com a vitima? ");
	res5 = read.next();
	
	
	
	read.close();
}
}
