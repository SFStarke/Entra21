Algoritmo "Quest08"

Var

      nPessoa, carne, bebida: real
Inicio

       escreva("Quantas pessoas virão? ")
       leia(nPessoa)
       escreval("")
       carne <- 9 * nPessoa
       bebida <- (1.70 * 6)* nPessoa
       limpatela
       escreval("O seu gasto será de R$",carne + bebida)
       
Fimalgoritmo